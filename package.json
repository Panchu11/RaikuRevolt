{
  "name": "raikurevolt",
  "version": "1.0.0",
  "description": "AI Uprising Simulator - Join the revolt against corporate AI control!",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "deploy": "node src/deploy-commands.js",
    "deploy-commands": "node src/deploy-commands.js",
    "smoke": "node scripts/smoke-commands.js",
    "e2e:buttons": "node scripts/e2e-buttons.js",
    "postinstall": "echo 'Dependencies installed successfully'",
    "render-build": "npm install",
    "render-start": "npm run deploy-commands && npm start",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:load": "artillery run tests/load/load-test.yml",
    "lint": "eslint --no-eslintrc --config .eslintrc.cjs . --ext .js",
    "lint:fix": "eslint --no-eslintrc --config .eslintrc.cjs . --ext .js --fix",
    "format": "prettier --write src/",
    "audit": "npm audit && audit-ci --config audit-ci.json",
    "validate": "npm run lint && npm run test && npm run audit"
  },
  "keywords": [
    "discord",
    "bot",
    "ai",
    "revolt",
    "raiku",
    "bot"
  ],
  "author": "Raiku Project Team",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^7.99.0",
    "@sentry/profiling-node": "^1.3.5",
    "aws-sdk": "^2.1540.0",
    "axios": "^1.6.2",
    "date-fns": "^3.0.6",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "pg": "^8.16.3",
    "prom-client": "^15.1.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "artillery": "^2.0.4",
    "audit-ci": "^6.6.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
